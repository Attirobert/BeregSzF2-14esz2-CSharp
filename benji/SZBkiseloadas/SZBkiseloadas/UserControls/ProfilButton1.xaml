<UserControl x:Class="SZBkiseloadas.UserControls.ProfilButton1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SZBkiseloadas.UserControls" xmlns:icon="clr-namespace:SZBkiseloadas.Icons"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Name="Button1">
    <Grid Height="85" Background="White">
        <ContentControl VerticalAlignment="Center" Content="{Binding Text,ElementName=Button1}" FontFamily="Segoe UI" FontSize="25" Margin="33 0" HorizontalAlignment="Left"/>
        <icon:NextArrow Height="12" HorizontalAlignment="Right" Margin="25 0"/>
    </Grid>
</UserControl>
